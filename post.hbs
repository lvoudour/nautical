{{!< default}}
{{#post}}
<article id="{{comment_id}}" class="{{post_class}}">
	<div class="post-stamp">{{date published_at format='MMMM D, YYYY'}}
  </div>
	<h1 class="post-title">{{title}}</h1>
	{{content}}
  {{#if tags}}<span class="tag-prefix">#</span>{{/if}}<span class="taglist">{{tags separator=" "}}</span>{{#if featured}}<span class="separator"> &middot; </span><i class="fa fa-star-o feature-star"></i>{{/if}}
  {{> comments}}
</article>  
{{/post}}